public class Main {
    public static void Main(String[] args) {
      int sum1 = 100 + 50;
      int sum2 = sum1 + 250;
      int sum3 = sum2 + sum2;
      int sum4 = sum3 + sum2;
      System.out.println(sum1);
      System.out.println(sum2);
      System.out.println(sum3); 
      System.out.println(sum4); 
    }
  }